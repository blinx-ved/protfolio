---
import LineItem from '../components/LineItem.astro';
const { title, lineItems } = Astro.props;
---

<section class="flex flex-col space-y-1">
    <div>
        <h1 class="font-medium text-xl">{title}</h1>
        <hr />
    </div>
    {lineItems.map((lineitem: any) => (
            <LineItem {...lineitem} />
    ))}
</section>